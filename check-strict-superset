# Enter your code here. Read input from STDIN. Print output to STDOUT
set_A = input()
number_of_inputs = int(input())
set_A = set_A.split(" ")
set_A = set(set_A)
count = 0
flag = True
while count < number_of_inputs:
    current_input_set = input().split(" ")
    for i in current_input_set:
        if i not in set_A:
            flag = False
    count += 1
print(flag)
