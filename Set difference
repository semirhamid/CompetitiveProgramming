# Enter your code here. Read input from STDIN. Print output to STDOUT
english_students = int(input())
english_learners = set(input().split())
french_students = int(input())
french_learners = set(input().split())

result = []
for i in english_learners:
    if i not in french_learners:
        result.append(i)
print(len(result))
